<div class="catItem-header" {{#if settings.color}}style="background-color:{{settings.color}}"{{/if}} title="{{name}}">
    <input type="text" value="{{name}}" {{#if notEdit}}disabled='disabled'{{/if}}/>
    <i class="{{#if visible}}icon-eye-open{{else}}icon-eye-close{{/if}} visibility" title="{{#if visible}}{{t "annotate.hide category"}}{{else}}{{t "annotate.show category"}}{{/if}}"></i>
    <i class="icon-pencil scale edit" title="{{t "annotate.edit category scaling"}}"></i>
    <i class="icon-trash delete edit"></i>
    {{#if settings.createdAsMine}}
        <!-- <i class="icon-public sharedVis edit" data-sharedVis='public'></i>
        <i class="icon-private sharedVis edit" data-sharedVis='private'></i>
        <i class="icon-shared-with-admin sharedVis edit" data-sharedVis='shared-with-admin'></i> -->
        <button
        type="button"
        class="sharedVisibility"
        data-trigger="click"
        title="
            <div class='sharedVisibility' data-id='{{id}}'>
                <button type='button' data-sharedVis='public'>
                    <i
                        class='icon-white icon-public sharedVis'
                        title='{{t "timeline.change visibility.to public"}}'
                    ></i>
                </button>
                <button type='button' data-sharedVis='private'>
                    <i
                        class='icon-white icon-private sharedVis'
                        title='{{t "timeline.change visibility.to private"}}'
                    ></i>
                </button>
                <button type='button' data-sharedVis='shared-with-admin'>
                    <i
                        class='icon-white icon-shared-with-admin sharedVis'
                        title='{{t "timeline.change visibility.to shared with admin"}}'
                    ></i>
                </button>
            </div>
            "
            class="button"
            data-placement="top"
        >
            <i
            title="{{t "timeline.change visibility.tooltip"}}"
            class="icon-dark-grey icon-{{access}} edit"
            ></i>
        </button>
    {{/if}}
    <i class="{{#if seriesCategoryId}}icon-series-category{{else}}icon-video-category{{/if}} toggleSeries edit" title="{{#if seriesCategoryId}}{{t "annotate.series category"}}{{else}}{{t "annotate.event category"}}{{/if}}"></i>
    <div class="colorpicker">
        <input id="color-{{name}}" type="text" class="colorpicker edit" value="{{#if settings}}{{settings.color}}{{/if}}" /></div>
    </div>
</div>
<div class="catItem-labels" {{#if settings.color}}style="background-color:{{settings.color}}"{{/if}}></div>
<div class="catItem-add edit"><i class="icon-plus-sign"></i>{{t "annotate.create a label"}}</div>
